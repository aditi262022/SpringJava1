<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="emp1" class="com.gqt.p1.Employee">
	<property name="empid"><value>1</value></property>
	<property name="empname"><value>Rahim</value></property>
	<property name="empcompany"><value>Wipro</value></property>
	<property name="empsalary"><value>50000</value></property>
</bean>
<bean id="emp2" class="com.gqt.p1.Employee">
	<property name="empid"><value>2</value></property>
	<property name="empname"><value>Rahul</value></property>
	<property name="empcompany"><value>IBM</value></property>
	<property name="empsalary"><value>40000</value></property>
</bean>
<bean id="emp3" class="com.gqt.p1.Employee">
	<property name="empid"><value>3</value></property>
	<property name="empname"><value>Rohan</value></property>
	<property name="empcompany"><value>TCS</value></property>
	<property name="empsalary"><value>45000</value></property>
</bean>
<bean id="emp4" class="com.gqt.p1.Employee">
	<constructor-arg value="4" type="int"></constructor-arg>
	<constructor-arg value="Ram"></constructor-arg>
	<constructor-arg value="Infosys"></constructor-arg>
	<constructor-arg value="55000" type="int"></constructor-arg>
</bean>
</beans>